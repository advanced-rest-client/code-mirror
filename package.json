{
  "name": "code-mirror",
  "version": "1.1.12",
  "license": "LICENSE.txt",
  "description": "CodeMirror web component made with Polymer",
  "scripts": {
    "serve": "node_modules/.bin/polymer serve",
    "lint": "node_modules/.bin/polymer lint code-mirror.html",
    "test-sauce": "node_modules/.bin/polymer test --plugin sauce --job-name \"code-mirror:local-test\"",
    "test": "node_modules/.bin/polymer test",
    "deps": "npm install --quiet && bower install --quiet",
    "update-deps": "npm update --quiet && bower update --quiet"
  },
  "keywords": [
    "web-component",
    "web-components",
    "polymer",
    "code-mirror",
    "code",
    "mirror",
    "chrome-apps"
  ],
  "author": "Pawel Psztyc <jarrodek@gmail.com>",
  "bugs": {
    "url": "https://github.com/advanced-rest-client/code-mirror/issues",
    "email": "arc@mulesoft.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/advanced-rest-client/code-mirror.git"
  },
  "devDependencies": {
    "polymer-cli": "^0.16.0"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "private": false
}
